<body>
    <div class="page-container">
        <div class="register-header">
            <a href="/login"><i class="fa fa-chevron-left" aria-hidden="true"></i></a>
            <p>Register for Stylo</p>
        </div>
        <form class="reg-info_form" name="regForm">
          First Name: <br>
          <input type="text" id="firstN-input" name="fn" class="reg-info__form__input" value="" required> <br> Last Name: <br>
          <input type="text" id="lastN-input" name="ln" class="reg-info__form__input" value="" required> <br> Username: <br>
          <input type="text" id="userN-input" name="un" class="reg-info__form__input" value="" required> <br> Password: <br>
          <input type="password" id="pass-input" name="pw" class="reg-info__form__input" value="" required> <br> Confirm Password: <br>
          <input type="password" id="pass-input-confirm" name="pw" class="reg-info__form__input" value="" required>
          <button type="button" id="reg-submit" class="reg-Button">Submit</button>
        </form>
    </div>
    <!--end page-container -->
    <script src="https://code.jquery.com/jquery.js"></script>
    <script src="js/bootstrap.min.js"></script>
    <script src="/js/index.js" charset="utf-8"></script>
    
